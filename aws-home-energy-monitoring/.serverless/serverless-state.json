        "dynamoDataStore": {
          "Type": "AWS::DynamoDB::Table",
          "Properties": {
            "TableName": "uas-home-energy-monitor",
            "AttributeDefinitions": [
              {
                "AttributeName": "primarykey",
                "AttributeType": "S"
              },
              {
                "AttributeName": "sortkey",
                "AttributeType": "N"
              }
            ],
            "KeySchema": [
              {
                "AttributeName": "primarykey",
                "KeyType": "HASH"
              },
              {
                "AttributeName": "sortkey",
                "KeyType": "RANGE"
              }
            ],
            "ProvisionedThroughput": {
